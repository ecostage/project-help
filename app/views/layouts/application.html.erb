<%= content_for :body do %>
  <%= render "layouts/nav" -%>

  <div id="main" class="container-fluid">
    <div class="row">
      <div class="col-sm-3 col-md-2 col-lg-2 sidebar">
        <%= render "layouts/sidebar" -%>
      </div>
      <div class="col-sm-9 col-md-10 col-lg-10">
        <%= render "layouts/flashes" -%>
        <%= yield %>
      </div>
    </div>
  </div>
<% end %>
<%= render template: "layouts/base" %>
